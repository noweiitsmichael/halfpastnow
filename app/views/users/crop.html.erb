<h2>Crop Profilepic</h2>
<% content_for (:head) do %>
<%= stylesheet_link_tag "jquery.Jcrop" %>
<%= javascript_include_tag "jquery.Jcrop.min" %>

<script type="text/javascript">
  $(function() {
    $('#cropbox').Jcrop();
  });
</script>

<%= image_tag @user.profilepic.url(:large), :id => "cropbox" %>

<% end %>

